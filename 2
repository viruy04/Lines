#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    printf("Введите строку: ");
    scanf("%s", str);

    int len = strlen(str);

    if (len > 5) {
        printf("Первые три символа: %.3s\n", str);
        printf("Последние три символа: %.3s\n", &str[len-3]);
    } else {
        for (int i = 0; i < len; i++) {
            printf("%c", str[0]);
        }
        printf("\n");
    }

    return 0;
}
